{
  "name": "dash-backend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "bun --env-file=../.env --watch src/index.ts",
    "build": "bun build src/index.ts --outdir=dist --target=bun",
    "start": "bun --env-file=../.env src/index.ts",
    "start:production": "bun --env-file=../.env src/db/init.ts && bun --env-file=../.env src/index.ts",
    "db:init": "bun --env-file=../.env src/db/init.ts",
    "db:migrate": "bun --env-file=../.env src/db/migrate.ts",
    "db:seed": "bun --env-file=../.env src/db/seed.ts",
    "drizzle:generate": "drizzle-kit generate --config ./drizzle.config.ts",
    "drizzle:migrate": "drizzle-kit migrate --config ./drizzle.config.ts"
  },
  "dependencies": {
    "@hono/node-server": "^1.12.0",
    "@hono/zod-validator": "^0.2.2",
    "@paralleldrive/cuid2": "^2.2.2",
    "bcrypt": "^5.1.1",
    "drizzle-orm": "^0.36.0",
    "hono": "^4.0.0",
    "hono-rate-limiter": "^0.5.0",
    "jsonwebtoken": "^9.0.2",
    "rss-parser": "^3.13.0",
    "systeminformation": "^5.27.14",
    "ws": "^8.18.3",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@libsql/client": "^0.14.0",
    "@types/bcrypt": "^5.0.2",
    "@types/bun": "latest",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/ws": "^8.18.1",
    "drizzle-kit": "^0.28.0"
  }
}
