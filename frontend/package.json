{
  "name": "dash-frontend",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "build:icons": "node scripts/build-icon-index.js",
    "cleanup:icons": "node scripts/cleanup-icons.js",
    "consolidate:themes": "node scripts/consolidate-themes.js",
    "check:schemes": "node scripts/check-new-schemes.js",
    "update:schemes": "node scripts/check-new-schemes.js --download && npm run consolidate:themes",
    "check:icons": "node scripts/check-new-icons.js",
    "update:icons": "node scripts/check-new-icons.js --download && npm run build:icons",
    "prestart": "concurrently \"npm run build:icons\" \"npm run consolidate:themes\"",
    "start": "ng serve --proxy-config proxy.conf.json",
    "dev": "ng serve --proxy-config proxy.conf.json",
    "prebuild": "concurrently \"npm run build:icons\" \"npm run consolidate:themes\"",
    "build": "npm run prebuild && ng build --output-path=dist/browser",
    "build:prod": "ng build --configuration production --output-path=dist/browser",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@10.9.2",
  "dependencies": {
    "@angular/animations": "^21.0.0",
    "@angular/cdk": "^21.0.2",
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "@casl/ability": "^6.7.3",
    "@fortawesome/angular-fontawesome": "^0.15.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fullcalendar/angular": "^6.1.20",
    "@fullcalendar/core": "^6.1.20",
    "@fullcalendar/daygrid": "^6.1.20",
    "@fullcalendar/interaction": "^6.1.20",
    "@tiptap/core": "^3.13.0",
    "@tiptap/extension-bubble-menu": "^3.13.0",
    "@tiptap/extension-bullet-list": "^3.13.0",
    "@tiptap/extension-floating-menu": "^3.13.0",
    "@tiptap/extension-list-item": "^3.13.0",
    "@tiptap/extension-ordered-list": "^3.13.0",
    "@tiptap/extension-placeholder": "^3.13.0",
    "@tiptap/starter-kit": "^3.13.0",
    "gridstack": "^12.3.3",
    "ngx-tiptap": "^14.0.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.2",
    "@angular/cli": "^21.0.2",
    "@angular/compiler-cli": "^21.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.10.1",
    "js-yaml": "^4.1.1",
    "jsdom": "^27.1.0",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}
